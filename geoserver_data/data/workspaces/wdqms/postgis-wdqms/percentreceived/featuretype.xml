      <virtualTable>
        <name>percentreceived</name>
        <sql>select geom,isempty,invola,indexnbr,SUM(nr_received) as nr_received,&#xd;
sum( CASE WHEN hasduplicate THEN 0 ELSE nr_expected END )  as nr_expected,&#xd;
SUM(nr_used) as nr_used,SUM(nr_rejected) as nr_rejected ,SUM(nr_blacklisted) as nr_blacklisted,SUM(nr_passive) as nr_passive,SUM(nr_missing) as nr_missing, AVG(per_received) as per_received, AVG(per_used) as per_used &#xd;
from stationsbyperiod &#xd;
where &#xd;
assimilationdate = &apos;%year%-%month%-%day% %hourperiod%:00:00&apos; and center=&apos;%center%&apos; and varid in (110,1)&#xd;
group by geom,isempty,invola,indexnbr
