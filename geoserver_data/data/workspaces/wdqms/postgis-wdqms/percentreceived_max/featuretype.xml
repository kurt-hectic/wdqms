        <sql>select geom,invola,indexnbr, max( per_received ) as per_received, max(nr_received) as nr_received, MIN(isempty::integer)::boolean as isempty&#xd;
from (&#xd;
select geom,isempty,invola,indexnbr,center,SUM(nr_received) as nr_received,&#xd;
sum( CASE WHEN hasduplicate THEN 0 ELSE nr_expected END )  as nr_expected,&#xd;
SUM(nr_used) as nr_used,SUM(nr_rejected) as nr_rejected ,SUM(nr_blacklisted) as nr_blacklisted,SUM(nr_passive) as nr_passive,SUM(nr_missing) as nr_missing, &#xd;
AVG(per_received) as per_received, AVG(per_used) as per_used &#xd;
from stationsbyperiod &#xd;
where &#xd;
assimilationdate = &apos;%year%-%month%-%day% %hourperiod%:00:00&apos; and varid in (110,1) &#xd;
