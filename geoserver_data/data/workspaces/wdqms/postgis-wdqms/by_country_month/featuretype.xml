      <virtualTable>
        <name>by_country_month</name>
        <sql>select row_number() OVER() as uid, ss.*, v.stationname, v.geom from vola v join (&#xd;
select invola, vola_id, center, indexnbr, avg(per_used) as per_used , avg(per_received) as per_received,  &#xd;
sum(nr_received) as nr_received,&#xd;
sum( CASE WHEN hasduplicate THEN 0 ELSE nr_expected END )  as nr_expected,&#xd;
sum(nr_used) as nr_used,sum(nr_rejected) nr_rejected,&#xd;
sum(nr_blacklisted) as nr_blacklisted, sum(nr_passive) as nr_passive,sum(nr_missing) nr_misssing &#xd;
from stationsbyperiod s &#xd;
  where assimilationdate between &apos;%year%-%month%-01&apos; and (&apos;%year%-%month%-&apos; || DATE_PART(&apos;days&apos;, DATE_TRUNC(&apos;month&apos;, &apos;%year%-%month%-01&apos;::timestamp ) + &apos;1 MONTH&apos;::INTERVAL - &apos;1 DAY&apos;::INTERVAL )::text)::date&#xd;
 and varid in (110,1) group by yyyy,mm,center,indexnbr, vola_id, invola ) ss on v.id=ss.vola_id&#xd;
where ( char_length(&apos;%countrycode%&apos;) = 0 or  v.isocc=&apos;%countrycode%&apos; ) and center = &apos;%center%&apos;
</sql>
